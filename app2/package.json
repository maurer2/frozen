{
  "name": "frozen-app",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "ts-node --esm './src/cli.tsx'",
    "start:watch": "tsx watch './src/cli.tsx'",
    "build": "tsc",
    "serve:dist": "node './dist/cli.js'",
    "test": "prettier --check . && xo && ava",
    "test:types": "tsc --noEmit --project './tsconfig.json'"
  },
  "dependencies": {
    "@inkjs/ui": "^1.0.0",
    "@sindresorhus/tsconfig": "^4.0.0",
    "@t3-oss/env-core": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vdemedes/prettier-config": "^2.0.1",
    "@zodios/core": "^10.9.6",
    "@zodios/react": "^10.4.5",
    "ava": "^5.3.1",
    "axios": "^1.5.1",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink": "^4.4.1",
    "ink-big-text": "^2.0.0",
    "ink-spinner": "^5.0.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "xo": "^0.56.0",
    "zod": "^3.22.4"
  },
  "files": [
    "dist"
  ],
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  }
}
